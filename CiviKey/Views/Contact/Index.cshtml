@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@*<script src="http://maps.googleapis.com/maps/api/js?client=72861695587.apps.googleusercontent.com&sensor=true&v=3.8" type="text/javascript"></script>*@
@*<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>*@

<script src="@Url.Content("~/Scripts/contacts.js")" type="text/javascript"></script>
<script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?key=AIzaSyCDAqSfJYM4GCTbN-dl-JLE4ux3XP7FdHs&sensor=true"></script>

<div class="contact-content">
    <div class="contact-title">
        Pour toute question sur l’utilisation de Civikey, si vous voulez participer au projet ou pour toute nouvelle idée de développement, n’hesitez pas à utiliser le formulaire ci-dessous pour nous contacter.
    </div>

    <div class="contact-main-content-back">
        <div class="contact-main-content">
            <div class="contact-form-map-container">
                <div class="contact-form">
                    <form action="POST">
                        <div class="contact-form-elem">
                            <label>A :</label> support@@invenietis.com
                        </div>
                        <div class="contact-form-elem">
                            <label>De :</label> <input type="text" name="email" />
                        </div>
                        <div class="contact-form-elem">
                            <label>Sujet :</label> <input type="text" name="email" />
                        </div>
                        <div class="contact-form-elem">
                            <label>Message :</label> <textarea type="text" name="email" ></textarea>
                        </div>
                        <div class="contact-form-elem-btn">
                            <input type="submit" value="Envoyer" class="send-btn"/>
                        </div>
                    </form>
                </div>
                <div class="contact-map">
                    <div id="map_canvas"></div>
                </div>
                <div style="clear:both;"></div>
            </div>
        </div>
    </div>
</div>